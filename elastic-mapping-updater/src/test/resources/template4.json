{
    "settings": {
        "index": {
            "number_of_shards": 5,
            "number_of_replicas": 0,
            "codec": "best_compression",
            "refresh_interval": "1s",
            "max_result_window": "1000000",
            "merge.scheduler.max_thread_count": 1
        },
        "analysis": {
            "char_filter": {
                "html_strip_filter": {
                    "type": "html_strip"
                }
            },
            "filter": {
                "million_token_limit": {
                    "type": "limit",
                    "max_token_count": 1000000
                },
                "period_break_filter": {
                    "type": "pattern_capture",
                    "preserve_original": false,
                    "patterns": [
                        "^([\\\\p{Nd},\\\\.'?]+)$|([^\\\\.']+)"
                    ]
                },
                "max_token_length_filter": {
                    "type": "length",
                    "max": 10922
                }
            },
            "analyzer": {
                "general_analyzer": {
                    "type": "custom",
                    "filter": [
                        "million_token_limit",
                        "max_token_length_filter",
                        "icu_normalizer"
                    ],
                    "char_filter": [ ],
                    "tokenizer": "icu_tokenizer"
                },
                "path_analyzer": {
                    "type": "custom",
                    "tokenizer": "path_analyzer",
                    "filter": [
                        "max_token_length_filter",
                        "icu_normalizer"
                    ]
                },
                "version_analyzer": {
                    "type": "pattern",
                    "pattern": "\\\\s+"
                },
                "standard_max_term_100": {
                    "type": "standard",
                    "stopwords": "_english_",
                    "max_token_length": 100
                }
            },
            "normalizer": {
                "keyword_normalizer": {
                    "type": "custom",
                    "filter": [
                        "icu_normalizer"
                    ]
                }
            },
            "tokenizer": {
                "path_analyzer": {
                    "type": "path_hierarchy",
                    "delimiter": "/"
                }
            }
        }
    },
    "mappings": {
        "_routing": {
            "required": true
        },
        "dynamic": "strict",
        "properties": {
            "FAILURES": {
                "properties": {
                    "AJP_JOB_RUN_ID": {
                        "type": "long",
                        "ignore_malformed": true
                    },
                    "AJP_WORK_UNIT_ID": {
                        "type": "keyword",
                        "ignore_above": 20
                    },
                    "COMPONENT": {
                        "type": "keyword",
                        "ignore_above": 64
                    },
                    "DATE": {
                        "type": "date",
                        "format": "strict_date_optional_time||epoch_second"
                    },
                    "ID": {
                        "type": "keyword",
                        "ignore_above": 32
                    },
                    "MESSAGE": {
                        "type": "text",
                        "copy_to": [
                            "FAILURES.MESSAGE_AGGREGATE"
                        ],
                        "analyzer": "general_analyzer"
                    },
                    "MESSAGE_AGGREGATE": {
                        "type": "keyword",
                        "ignore_above": 256
                    },
                    "STACK": {
                        "type": "text",
                        "analyzer": "general_analyzer"
                    },
                    "WORKFLOW_ACTION": {
                        "type": "keyword",
                        "ignore_above": 32
                    },
                    "WORKFLOW_NAME": {
                        "type": "keyword",
                        "ignore_above": 100
                    }
                }
            },
            "PERSON": {
                "properties": {
                    "NAME": {
                        "type": "keyword",
                        "ignore_above": 20
                    },
                    "AGE": {
                        "type": "long",
                        "ignore_malformed": true
                    }
                }
            },
            "HOLD_DETAILS": {
                "type": "nested",
                "properties": {
                    "FIRST_HELD_DATE": {
                        "type": "date",
                        "format": "strict_date_optional_time||epoch_second"
                    },
                    "HOLD_HISTORY": {
                        "type": "date",
                        "format": "strict_date_optional_time||epoch_second"
                    },
                    "HOLD_ID": {
                        "type": "text"
                    }
                }
            },
            "ID": {
                "type": "long",
                "ignore_malformed": true
            },
            "TITLE": {
                "type": "text",
                "analyzer": "path_analyzer"
            },
            "CONTENT_PRIMARY": {
                "type": "text",
                "analyzer": "path_analyzer"
            },
            "IS_HEAD_OF_FAMILY": {
                "type": "long",
                "ignore_malformed": true
            },
            "DATE_PROCESSED": {
                "type": "date",
                "format": "strict_date_optional_time||epoch_second"
            },
            "DATE_DISPOSED": {
                "type": "date",
                "format": "strict_date_optional_time||epoch_second"
            }
        }
    },
    "index_patterns": [
        "*_acmesample-*"
    ]
}
